page {
  font-family: Arial, sans-serif;
  margin: 0;
  padding: 0;
  position: relative;
  background: linear-gradient(144.56deg, rgba(255, 255, 255, 0.6) 16.73%, rgba(230, 232, 237, 0.6) 85.71%);
}

.page-background {
  position: relative; /* 确保子元素的绝对定位是相对于该元素 */
  width: 100%; /* 容器宽度，根据需要调整 */
  height: 100%; /* 容器高度，根据需要调整 */
}

.navbar {
  display: flex;
  justify-content: space-around;
  padding: 5px;
}

.title {
  font-size: 18px;
  text-align: center;
  color: #00093b63;
}
.title.active{
  color: #29354F;
}

.dot {
  width: 8px; /* 圆点大小 */
  height: 8px; /* 圆点大小 */
  border-radius: 5px; /* 圆点半径 */
  background-color: #00093B38; /* 非活动圆点颜色 */
  margin: 0 5px; /* 圆点间隔 */
}
.dot.active {
  background-color: #29354F;
}